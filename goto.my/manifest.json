{
  "manifest_version": 2,

  "name": "goto.my",
  "description": "Custom URLs",
  "version": "0.6.2",

  "omnibox": {"keyword": "g"},

  "options_page": "options.html",

  "permissions": [
      "tabs",
      "storage"
  ],

  "background": {
    "scripts": ["goto.js"]
  },

  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
